<property name="prop:Pause after execution" class="ptolemy.kernel.util.ConfigurableAttribute"><configure>if true then the execution is paused after the execution of the recipe</configure></property>
<property name="prop:Allow empty inputs" class="ptolemy.kernel.util.ConfigurableAttribute"><configure>if true then an empty sof on the input port will be broadcast to the output without executing the recipe</configure></property>
